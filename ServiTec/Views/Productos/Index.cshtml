@model List<ServiTec.Database.Models.Producto>
@{
    ViewBag.Title = "Index";
}

<h2>Productos</h2>


<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Código</th>
            <th>Descripción</th>
            <th>Precio</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var producto in Model)
        {
        <tr>
            <td>@producto.Id</td>
            <td>@producto.Descripcion</td>
            <td>@producto.Precio</td>
        </tr>
        }
    </tbody>
</table>
